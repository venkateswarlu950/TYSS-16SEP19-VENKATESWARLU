package com.tyss.springcore.di;

public class Hello {
	
	private String msg;

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}
	
	public void init() {
		System.out.println("init method excuted");
	}
	public void destroy() {
		System.out.println("destroy method exuted");
		
	}
	
	

}
