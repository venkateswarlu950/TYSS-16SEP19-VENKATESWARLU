package com.tyss.hibernatewithjpa.dto;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="product")
public class ProductInfo {
	
	private int id;
	
	private String name;
	
	private int qua;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getQua() {
		return qua;
	}

	public void setQua(int qua) {
		this.qua = qua;
	}
	
	

}
